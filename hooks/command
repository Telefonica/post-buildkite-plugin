#!/usr/bin/env bash

set -uo pipefail
set +e

env | sort

echo " COMIENZA LA EJECUCION "
echo " ------------- "

eval "${BUILDKITE_BUILD_CHECKOUT_PATH}/${BUILDKITE_COMMAND}"

exit_code=$?

echo " ------------- "

if [ $exit_code -ne 0 ]; then
  echo "Ha petado"
  exit $exit_code
fi

echo "Palante"

